config ipset 'vpn_en'
	list name 'vpn_en4'
	list name 'vpn_en6'
	list domain 'ua'
	list domain 'linkedin.com'
	list domain 'arkoselabs.com'
	list domain 'twirpx.com'
	list domain 'btt.cmacapps.com'
	list domain 'underver.se'
