	list domain 'twitter.com'
	list domain 'twimg.com'
	list domain 'facebook.com'
	list domain 'fbcdn.net'
	list domain 'meduza.io'
	list domain 'bbc.com'
	list domain 'the-village.ru'
	list domain 'instagram.com'
	list domain 'cdninstagram.com'
	list domain 'zona.media'
	list domain 'azattyq.org'
